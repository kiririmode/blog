
{
  "filters": {
    "comments": true,
    "whitelist": {
      "allow": [
        "/スクラムマスター/",
        "/\\[(https?|tex):.*\\]/",
        "デグレード",
        "一つ一つ",
        "幸か不幸か",
        "/^> .*$/",
        "mysql",
        "[wikipedia",
        "同時確率密度関数",
        "周辺確率密度関数",
        "/\\$\\$[^]*?\\$\\$/m",
        "/\\$[^]*?\\$/",
        "改正個人情報保護法",
        "個人情報保護委員会",
        "個人情報取扱事業者",
        "個人関連情報取扱事業者",
        "改正個人情報保護法",
        "クッキー",
        "git",
        "どこもかしこも",
        "非負値可測関数列"
      ]
    }
  },
  "rules": {
    "preset-ja-technical-writing": {
      "no-exclamation-question-mark": false,
      "max-kanji-continuous-len": {
        "max": 7,
        "allow": ["事前課題解決力"]
      }
    },
    "spellcheck-tech-word": true,
    "textlint-rule-preset-ja-spacing": true,
    "prh": {
      "rulePaths": ["./prh.yml"]
    }
  }
}
